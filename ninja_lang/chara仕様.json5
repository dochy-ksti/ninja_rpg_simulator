{
  ID: "foo",
  v: [
      [ {v:"基本目標", n:"何もなくても目標にする"},//条件無しで存在する基本目標
        {v:"b:情報を隠す", n:"さあ目標はなんでしょう", c: "f:偽起動条件:条件"},
      ],
      //aはany,条件を満たせば上のやつをふっとばして、vを上書きする。a以外のvは必要かわからない
      [ {v:"a:目標ID", d: "説明...いるか？名前で良くない？", n: "名前", c:"条件"},
        //無目的に上書きすることでディアクティベート
        {v:"a:", d: "ディアクティベーション", n: "目標を失う", c: "条件"},
        {v:"a:最初の目標ID", n:"いろいろ知る前の目標", c: "条件" },
        {v:"b:情報を隠す", n:"新目標出現には何が必要でしょうか", c: "b:条件隠しテキスト:条件"},
      ],
      [ {v: "a:目標2", n: "下のセクションが優先される",c: "条件"}, //条件がなくてもいいのは一番上だけ
        {v: "b:", c: "条件"}, //空文字列なので第2目標の存在がAI上見えない

      ],
  ]
}