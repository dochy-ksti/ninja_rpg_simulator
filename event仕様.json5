    ID:"foo",
    v:[
        [
            {   d:"説明あああああああああああああ",
                n:"一番上はメインイベント名",v:"a/30"},
            {   n:"???", d:"どうすればこのイベントは起動するかな?",
                c:"b:起動条件のヒント:起動イベントid"},
        ],[
            {   d:"別クリア条件、またの名を弱点、の説明",
                n:"弱点名",v:-10, c:"弱点起動条件"},
        ],[
            {   d:"description",
                n:"別弱点",v: -10,c:"条件"},
            {   v:"f:a/10"},
            {   d:"偽記述", //AI用fakeと人間用偽メッセージの同時設定
                n:"偽弱点名",v:"d:人間用ニセメッセージ",c:"fake解除条件"},
            {   d:"直列弱点",
                n:"弱点名", v:"k/20",c:"a/起動条件"},
            {   d:"にせ弱点", //不可能条件を指定し情報を隠されても、a/の起動条件を満たせば下のも起動して消える
                n:"にせ弱点名",v:"b:なんとかかんとか",c:"不可能条件"}
        ]
    ],
    //ownタイプ。
    //all:一度実行されると全員が所有権を持つ。もう実行できない
    //one:最初に実行した一人が所有権を持つ。もう実行できない
    //none:誰も所有権を持たない。何度でも実行できる
    //unlimited:誰かが所有権を得ても、また別の人は実行でき、実行すれば所有権が得られる
    own:"all",

    //これらのイベントが実行されると、このイベントも即時実行される
    chain:["e1","e2"],
    //自分が持っているone,unlimitedのイベントを渡す
    give:{c: "chara", i: "event"},
    //相手が持っているone,unlimitedのイベントを奪う
    take: {c:"chara",i:"event"},
    //freeの場合、行動回数を消費しないで実行可能
    free: true,


}